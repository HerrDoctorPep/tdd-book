[Unit]
Description=Gunicorn server for DOMAIN 

[Service]
Restart=on-failure
User=pubuntu
WorkingDirectory=/home/pubuntu/sites/DOMAIN
EnvironmentFile=/home/pubuntu/sites/DOMAIN/.env

ExecStart=/home/pubuntu/anaconda3/envs/tdd-book/bin/gunicorn \
    --bind unix:/tmp/DOMAIN.socket superlists.wsgi:application

[Install]
WantedBy=multi-user.target

